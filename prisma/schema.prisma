generator client {
  provider = "prisma-client-js"
  output   = "../src/generated/prisma"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Users {
  id       Int     @id @default(autoincrement())
  username String  @unique
  password String
  age      Int
  City     String
  todos    Todos[]
}

model Todos {
  id          Int     @id @default(autoincrement())
  title       String
  description String
  isComplted  Boolean
  userId      Int
  user        Users   @relation(fields: [userId], references: [id])
}
